{
  "dashboard": {
    "title": "Fibonacci Service Metrics",
    "tags": ["fibonacci", "rust", "prometheus"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 0,
    "refresh": "5s",
    "panels": [
      {
        "id": 1,
        "title": "Request Rate (req/s)",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        },
        "targets": [
          {
            "expr": "rate(requests_total[1m])",
            "legendFormat": "Request Rate",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "reqps",
            "label": "Requests/sec"
          },
          {
            "format": "short"
          }
        ]
      },
      {
        "id": 2,
        "title": "HTTP Status Codes (200, 400, 429, 500)",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        },
        "targets": [
          {
            "expr": "rate(request_status_codes_total{code=\"200\"}[1m])",
            "legendFormat": "200 OK",
            "refId": "A"
          },
          {
            "expr": "rate(request_status_codes_total{code=\"400\"}[1m])",
            "legendFormat": "400 Bad Request",
            "refId": "B"
          },
          {
            "expr": "rate(request_status_codes_total{code=\"429\"}[1m])",
            "legendFormat": "429 Too Many Requests",
            "refId": "C"
          },
          {
            "expr": "rate(request_status_codes_total{code=\"500\"}[1m])",
            "legendFormat": "500 Internal Server Error",
            "refId": "D"
          }
        ],
        "yaxes": [
          {
            "format": "reqps",
            "label": "Requests/sec"
          },
          {
            "format": "short"
          }
        ]
      },
      {
        "id": 3,
        "title": "Request Duration - P99, P95, P50",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.99, rate(request_duration_seconds_bucket[1m]))",
            "legendFormat": "P99 Latency",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(request_duration_seconds_bucket[1m]))",
            "legendFormat": "P95 Latency",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.50, rate(request_duration_seconds_bucket[1m]))",
            "legendFormat": "P50 Latency (Median)",
            "refId": "C"
          }
        ],
        "yaxes": [
          {
            "format": "s",
            "label": "Duration"
          },
          {
            "format": "short"
          }
        ]
      },
      {
        "id": 4,
        "title": "Active Requests",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        },
        "targets": [
          {
            "expr": "active_requests",
            "legendFormat": "Active Requests",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "short",
            "label": "Requests"
          },
          {
            "format": "short"
          }
        ]
      },
      {
        "id": 5,
        "title": "Total Requests",
        "type": "stat",
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 0,
          "y": 16
        },
        "targets": [
          {
            "expr": "requests_total",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto",
          "orientation": "auto",
          "reduceOptions": {
            "calcs": ["lastNotNull"],
            "fields": "",
            "values": false
          },
          "textMode": "auto"
        },
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 100
                },
                {
                  "color": "red",
                  "value": 1000
                }
              ]
            }
          }
        }
      },
      {
        "id": 6,
        "title": "Success Rate (%)",
        "type": "stat",
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 6,
          "y": 16
        },
        "targets": [
          {
            "expr": "sum(rate(request_status_codes_total{code=\"200\"}[1m])) / sum(rate(request_status_codes_total[1m])) * 100",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto",
          "orientation": "auto",
          "reduceOptions": {
            "calcs": ["lastNotNull"],
            "fields": "",
            "values": false
          },
          "textMode": "auto"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "red",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 95
                },
                {
                  "color": "green",
                  "value": 99
                }
              ]
            }
          }
        }
      },
      {
        "id": 7,
        "title": "Average Response Time",
        "type": "stat",
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 12,
          "y": 16
        },
        "targets": [
          {
            "expr": "rate(request_duration_seconds_sum[1m]) / rate(request_duration_seconds_count[1m])",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto",
          "orientation": "auto",
          "reduceOptions": {
            "calcs": ["lastNotNull"],
            "fields": "",
            "values": false
          },
          "textMode": "auto"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 0.1
                },
                {
                  "color": "red",
                  "value": 1
                }
              ]
            }
          }
        }
      },
      {
        "id": 8,
        "title": "Rate Limit Reached Count",
        "type": "stat",
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 18,
          "y": 16
        },
        "targets": [
          {
            "expr": "request_limit_reached_total",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto",
          "orientation": "auto",
          "reduceOptions": {
            "calcs": ["lastNotNull"],
            "fields": "",
            "values": false
          },
          "textMode": "auto"
        },
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "orange",
                  "value": 1
                }
              ]
            }
          }
        }
      },
      {
        "id": 9,
        "title": "Response Size Distribution",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 20
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.99, rate(response_size_bytes_bucket[1m]))",
            "legendFormat": "P99 Response Size",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, rate(response_size_bytes_bucket[1m]))",
            "legendFormat": "P50 Response Size",
            "refId": "B"
          }
        ],
        "yaxes": [
          {
            "format": "bytes",
            "label": "Size"
          },
          {
            "format": "short"
          }
        ]
      },
      {
        "id": 10,
        "title": "Fibonacci Input Distribution (N values)",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 20
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.99, rate(fibonacci_n_distribution_bucket[1m]))",
            "legendFormat": "P99 Input N",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, rate(fibonacci_n_distribution_bucket[1m]))",
            "legendFormat": "P50 Input N",
            "refId": "B"
          }
        ],
        "yaxes": [
          {
            "format": "short",
            "label": "N Value"
          },
          {
            "format": "short"
          }
        ]
      }
    ]
  },
  "overwrite": true
}

